<h1>My events</h1>
<table class="table">
  <tr>
    <th>Subject</th>
    <th>Start</th>
    <th>End</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event['subject'] %></td>
      <td><%= event['start']['dateTime'] %></td>
      <td><%= event['end']['dateTime'] %></td>
    </tr>
  <% end %>
</table>
</br>
<%= link_to 'Calendar View', calendar_view_index_path %>
